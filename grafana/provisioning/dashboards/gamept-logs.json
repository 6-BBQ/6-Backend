{
  "dashboard": {
    "id": null,
    "title": "GamePT Application Logs",
    "tags": ["gamept", "logs"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "Application Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{application=\"gamept\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 8,
          "w": 24,
          "x": 0,
          "y": 0
        },
        "options": {
          "showTime": true,
          "showLabels": true,
          "showCommonLabels": false,
          "wrapLogMessage": true,
          "prettifyLogMessage": false,
          "enableLogDetails": true,
          "dedupStrategy": "none",
          "sortOrder": "Descending"
        }
      },
      {
        "id": 2,
        "title": "Log Level Distribution",
        "type": "stat",
        "targets": [
          {
            "expr": "count by (level) (count_over_time({application=\"gamept\"}[5m]))",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 4,
          "w": 12,
          "x": 0,
          "y": 8
        }
      },
      {
        "id": 3,
        "title": "Error Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{application=\"gamept\", level=\"ERROR\"}",
            "refId": "A"
          }
        ],
        "gridPos": {
          "h": 6,
          "w": 24,
          "x": 0,
          "y": 12
        }
      }
    ],
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "refresh": "5s"
  }
}
